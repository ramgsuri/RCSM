<mapping>
	<class name="com.fil.rcsm.model.jaxb.FlightInfo">

		<map-to xml="FlightInfo" />

		<field name="flightNo" type="string">
			<bind-xml name="FlightNo" node="element" />
		</field>

		<field name="timestamp" type="long">
			<bind-xml name="Timestamp" node="element" />
		</field>

		<field name="modelNo" type="string">
			<bind-xml name="FlightModelNo" node="element" />
		</field>

	</class>
</mapping>